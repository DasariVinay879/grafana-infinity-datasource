{
  "name": "grafana-infinity-datasource",
  "version": "2.1.0",
  "description": "JSON, CSV, XML, GraphQL, HTML and REST API datasource for Grafana. Do infinite things with Grafana. Transform data with UQL/GROQ. Visualize data from many apis, RSS/ATOM feeds directly",
  "main": "dist/module.js",
  "scripts": {
    "build:backend": "mage -v",
    "build:backend:turbo": "yarn build:backend",
    "build:frontend": "yarn build",
    "build:frontend:turbo": "yarn build:frontend",
    "build:turbo": "yarn turbo build:backend:turbo build:frontend:turbo",
    "dev": "npm_config_yes=true npx @grafana/toolkit@8.4.7 plugin:dev",
    "watch": "npm_config_yes=true npx @grafana/toolkit@8.4.7 plugin:dev --watch",
    "start": "yarn watch",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "lint": "yarn spellcheck && yarn audit-ci",
    "test": "TZ=UTC yarn lint && jest --passWithNoTests --maxWorkers 4",
    "e2e": "yarn exec cypress run",
    "e2e:open": "yarn exec cypress open",
    "audit-ci": "yarn audit",
    "build": "NODE_OPTIONS=--openssl-legacy-provider npm_config_yes=true npx @grafana/toolkit@8.4.7 plugin:build --skipLint --skipTest",
    "ci-build": "yarn lint && NODE_OPTIONS=--openssl-legacy-provider npm_config_yes=true npx @grafana/toolkit@8.4.7 plugin:build",
    "sign": "NODE_OPTIONS=--openssl-legacy-provider npm_config_yes=true npx @grafana/toolkit@8.4.7 plugin:sign"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yesoreyeram/grafana-infinity-datasource.git"
  },
  "keywords": [
    "grafana",
    "grafana infinity",
    "grafana utils",
    "inline",
    "random walk",
    "json",
    "simple-json",
    "graphql",
    "csv",
    "html",
    "xml",
    "uql",
    "groq",
    "url",
    "scraping",
    "web scraping",
    "soap",
    "atom",
    "rss",
    "feed"
  ],
  "author": "Sriramajeyam Sugumaran",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/yesoreyeram/grafana-infinity-datasource/issues"
  },
  "homepage": "https://yesoreyeram.github.io/grafana-infinity-datasource",
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "@grafana/data": "10.2.0",
    "@grafana/runtime": "10.2.0",
    "@grafana/ui": "10.2.0",
    "cheerio": "^1.0.0-rc.10",
    "csv-parse": "^4.12.0",
    "groq-js": "1.1.8",
    "json-to-pretty-yaml": "^1.2.2",
    "jsonata": "2.0.2",
    "jsonpath-plus": "7.2.0",
    "lodash": "^4.17.21",
    "mathjs": "11.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rxjs": "6.6.3",
    "uql": "0.0.19",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@swc/core": "^1.3.95",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/cheerio": "^0.22.33",
    "@types/jest": "^29.5.6",
    "@types/xml2js": "^0.4.13",
    "cspell": "^7.3.8",
    "cypress": "^13.3.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "turbo": "^1.10.16"
  },
  "resolutions": {
    "xml2js": "0.6.2"
  }
}
