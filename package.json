{
  "name": "grafana-infinity-datasource",
  "version": "2.3.1",
  "description": "JSON, CSV, XML, GraphQL, HTML and REST API datasource for Grafana. Do infinite things with Grafana. Transform data with UQL/GROQ. Visualize data from many apis, RSS/ATOM feeds directly",
  "main": "dist/module.js",
  "scripts": {
    "build:backend": "mage -v",
    "build:backend:turbo": "yarn build:backend",
    "build:frontend": "yarn build",
    "build:frontend:turbo": "yarn build:frontend",
    "build:turbo": "yarn turbo build:backend:turbo build:frontend:turbo",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "start": "yarn watch",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "lint": "yarn spellcheck && yarn audit-ci",
    "test": "TZ=UTC yarn lint && TZ=UTC grafana-toolkit plugin:test",
    "e2e": "yarn exec cypress run",
    "e2e:open": "yarn exec cypress open",
    "build": "grafana-toolkit plugin:build",
    "ci-build": "yarn lint && grafana-toolkit plugin:ci-build",
    "audit-ci": "audit-ci",
    "sign": "grafana-toolkit plugin:sign"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grafana/grafana-infinity-datasource.git"
  },
  "keywords": [
    "grafana",
    "grafana infinity",
    "grafana utils",
    "inline",
    "random walk",
    "json",
    "simple-json",
    "graphql",
    "csv",
    "html",
    "xml",
    "uql",
    "groq",
    "url",
    "scraping",
    "web scraping",
    "soap",
    "atom",
    "rss",
    "feed"
  ],
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/grafana/grafana-infinity-datasource/issues"
  },
  "homepage": "https://grafana.github.io/grafana-infinity-datasource",
  "dependencies": {
